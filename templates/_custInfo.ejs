            <dd class="mul-dd">
                <div class="radio">
                    <ul id="isChild" class="clear">
                        <li class="<%=isChild=='1'?'checked':''%>" data-code="1">有</li>
                        <li class="<%=isChild=='2'?'checked':''%>" data-code="2">无</li>
