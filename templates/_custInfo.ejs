            <dt>有无当地社保</dt>
            <dd class="mul-dd">
                <div class="radio">
                    <ul id="isLocalSecurity" class="clear">
                        <li class="<%=isLocalSecurity=='1'?'checked':''%>" data-code="1">有</li>
                        <li class="<%=isLocalSecurity=='2'?'checked':''%>" data-code="2">无</li>
                    </ul>
                </div>
            </dd>
        </dl>
        <dl class="clear">
