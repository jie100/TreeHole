                        <div class="select-option">
                        </div>
                    </div>
                    <div class="select w-short">
                        <div id="custRegisterCity" class="selectedCont"><span data-code="<%=rCityCode%>" class="<%=rCityName?'':'gray'%>"><%=rCityName?rCityName:'市'%></span><a href="javascript:;" class="areat icon"></a></div>
                        <div class="select-option">
